struct person {
  name,
}

impl person {
  fn say(self, prefix, suffix) {
    self.say_name(prefix, suffix);
    return 0;
  }
  fn say_name(self, prefix, suffix) {
    print prefix ++ self.name ++ suffix;
    return 0;
  }
}

fn main() {
  p = person { name: "John" };
  prefix = "Hello, ";
  suffix = ".";
  p.say(prefix, suffix);
  return 0;
}